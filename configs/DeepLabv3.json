{
  "name": "DeepLabv3_r50_RF_Lovasz",
  "mode": "training",
  "manager": "DeepLabv3",
  "graph": {
    "model": "DeepLabv3",
    "backbone": "resnet50",
    "aspp": {"channels": 256},
    "out_stride": 8,
    "pretrained": true,
    "ss_pretrained_":"moco"
  },

  "data": {
    "experiment": 2,
    "use_relabeled": false,
    "blacklist": false,
    "transforms": ["pad", "flip", "blur", "colorjitter"],
    "split": 2,
    "batch_size": 8,
    "repeat_factor": [0],
    "repeat_factor_freq_thresh": 0.15
  },

  "loss": {"name":  "LovaszSoftmax"},

  "train": {
    "learning_rate": 0.0001,
    "lr_decay_gamma": 0.96,
    "epochs": 50
  },
  "log_every_n_epochs": 25,
  "cuda": true,
  "gpu_device": 0,
  "seed": 0
}